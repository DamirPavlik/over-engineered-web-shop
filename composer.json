{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "doctrine/dbal": "^4.0",
        "doctrine/migrations": "^3.8",
        "doctrine/orm": "^3.2",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.7",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.4",
        "symfony/twig-bridge": "^7.1",
        "symfony/webpack-encore-bundle": "1.15",
        "twig/intl-extra": "^3.10",
        "twig/twig": "^3.10",
        "vlucas/phpdotenv": "^5.6"
    }
}
